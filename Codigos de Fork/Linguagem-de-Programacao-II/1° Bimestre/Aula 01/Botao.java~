public class Botao
{
	private cor;
	private nome;
	public Botao()
	{
		nome = "";
		cor = "";
	}

	public void getCor(){return cor;}

	public void setColor(String cor){this.cor = cor;}

	public void dePress(){System.out.println("Pressionando botao");}

	public void unPress(){System.out.println("Despressionando botão");}

	public void setNome(Stringn nome){this.nome = nome;}

	public String getNome(){return nome;}

}

