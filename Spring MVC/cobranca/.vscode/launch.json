{
    "configurations": [
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Executar Extens√£o",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm"
        },

        {
            "type": "java",
            "name": "Spring Boot-CobrancaApplication<cobranca>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.algaworks.cobranca.CobrancaApplication",
            "projectName": "cobranca",
            "args": ""
        }
    ]
}